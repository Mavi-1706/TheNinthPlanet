<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFEDED"
    android:backgroundTint="#000000">

    <LinearLayout
        android:id="Layout"
        android:layout_width="411dp"
        android:layout_height="62dp"
        android:backgroundTint="#C9F4F0F0"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="96dp"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:contentDescription="@android:string/no"
            app:srcCompat="@drawable/logo_2870" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="88dp"
            android:layout_height="79dp"
            android:backgroundTint="#FFFFFF"
            android:contentDescription="@android:string/no"
            android:translationX="330dp"
            android:translationY="-69dp"
            app:srcCompat="@drawable/menu_FILL0_wght200_GRAD0_opsz24" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="94dp"
            android:layout_height="76dp"
            android:backgroundTint="#FFFFFF"
            android:translationX="90dp"
            android:translationY="-147dp"
            app:srcCompat="@drawable/public_FILL0_wght300_GRAD0_opsz24" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="74dp"
            android:layout_height="78dp"
            android:backgroundTint="#FFFFFF"
            android:translationX="176dp"
            android:translationY="-223dp"
            app:srcCompat="@drawable/smart_toy_FILL0_wght300_GRAD0_opsz24" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="100dp"
            android:layout_height="77dp"
            android:backgroundTint="#FFFFFF"
            android:translationX="242dp"
            android:translationY="-301dp"
            app:srcCompat="@drawable/satellite_alt_FILL0_wght300_GRAD0_opsz24" />


    </LinearLayout>

    <TableRow
        android:id="@+id/tableRow"
        android:layout_width="412dp"
        android:layout_height="67dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:translationY="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097">

        <ImageButton
            android:id="@+id/imageButton13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#FFFEFE"
            android:contentDescription="@android:string/no"
            android:translationY="30dp"
            app:srcCompat="@drawable/mms_FILL0_wght200_GRAD0_opsz24" />

        <ImageButton
            android:id="@+id/imageButton14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#FFFEFE"
            android:contentDescription="@android:string/no"
            android:translationX="310dp"
            app:srcCompat="@drawable/more_vert_FILL0_wght200_GRAD0_opsz24" />

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="196dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="12sp"
            android:translationX="-25dp"
            android:translationY="20dp"
            tools:text="¿Qué esta sucediendo?" />
    </TableRow>

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="570dp"
        android:background="#FBF8F8"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.987">

        <LinearLayout
            android:layout_width="352dp"
            android:layout_height="225dp"
            android:background="#FAF9F9"
            android:backgroundTint="#FFFDFD"
            android:orientation="vertical"
            android:translationX="30dp"
            android:translationY="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:background="#000000"
                android:backgroundTint="#090909"
                android:orientation="vertical"
                android:translationY="-0dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="#000000"
                android:orientation="vertical"
                android:translationY="140dp"></LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="352dp"
            android:layout_height="225dp"
            android:background="#FAF9F9"
            android:backgroundTint="#FDFDFD"
            android:orientation="vertical"
            android:translationX="30dp"
            android:translationY="30dp">

            <View
                android:id="@+id/divider"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                android:translationY="185dp" />

            <ImageButton
                android:id="@+id/imagebuttonfavorite"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#FBFBFB"
                android:contentDescription="@android:string/no"
                android:translationX="260dp"
                android:translationY="180dp"
                app:srcCompat="@drawable/favorite_FILL0_wght100_GRAD0_opsz24" />

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#FBFBFB"
                android:contentDescription="@android:string/no"
                android:translationX="300dp"
                android:translationY="137dp"
                app:srcCompat="@drawable/comment_FILL0_wght100_GRAD0_opsz24" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="55dp"
                android:layout_height="47dp"
                android:contentDescription="@android:string/no"
                android:translationY="-90dp"
                app:srcCompat="@drawable/austronautita" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#FFFFFF"
                android:contentDescription="@android:string/no"
                android:translationX="300dp"
                android:translationY="-130dp"
                app:srcCompat="@drawable/more_vert_FILL0_wght200_GRAD0_opsz24" />

            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:translationY="-180dp"
                app:srcCompat="@drawable/ESCUDOCECYT" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="213dp"
                android:layout_height="35dp"
                android:translationX="60dp"
                android:translationY="-210dp"
                tools:text="CECyT 9 &quot;Juan de Dios Batiz&quot;" />

            <View
                android:id="@+id/divider4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                android:translationY="-210dp" />

            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="371dp"
                android:layout_height="122dp"
                android:contentDescription="@android:string/paste"
                android:translationY="-200dp"
                tools:srcCompat="@tools:sample/backgrounds/scenic" />

        </LinearLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>